{
  "compilerOptions": {
    "declaration": false,
    "lib": [
      "es6",
      "es2016",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "rootDir": ".",
    "sourceMap": false,
    "target": "es2015",
    "stripInternal": false,
    "baseUrl": "",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "types": [
      "node"
    ],
    "outDir": "../gulp-compiled"
  }
}