{
  "AllowedHosts": "*",
  "SingleDomainApp": false,
  "enableSimpleMembership": "false",
  "ClientValidationEnabled": "true",
  "UnobtrusiveJavaScriptEnabled": "true",
  "autoFormsAuthentication": "false",
  "webpages:Version": "3.0.0.0",
  "webpages:Enabled": "false",
  "PreserveLoginUrl": "true",
  "aspnet:UseTaskFriendlySynchronizationContext": "true",
  "appContext.SetSwitch:Switch.System.Globalization.NoAsyncCurrentCulture": "true",
  "WebpackDevServer": "https://localhost:8896/",

  "DynaCon": {
    "Host": "http://api.dynacon.prod.env.works",
    "NetworkTimeout": "00:01:00",
    "ChangesPollingInterval": "00:02:00",
    "Parameters": {
      "skipDelay": "true"
    }
  },
  "autofac": {},
  "SemanticLoggingFileSink": {
    "Enabled": "true",
    "Type": "app",
    "Path": "D:\\LogFiles\\gantry\\app.log",
    "RollOnFileSizeLimit": "true",
    "FileSizeLimitBytes": "524288000",
    "RetainedFileCountLimit": "10",
    "Shared": "true",
    "BufferSize": "10"
  },
  "SemanticLoggingHealthFileSink": {
    "Enabled": "true",
    "Type": "health",
    "Path": "D:\\LogFiles\\gantry\\health.log",
    "RollOnFileSizeLimit": "true",
    "FileSizeLimitBytes": "524288000",
    "RetainedFileCountLimit": "10",
    "Shared": "true",
    "BufferSize": "1"
  },
  "MachineKey": {
    "DecryptionMethod": "TripleDES",
    "DecryptionKey": "CD8459A19C542E01971B4F921C7DED5C9503FA32F39613BE",
    "ValidationMethod": "SHA1",
    "ValidationKey": "8C9A9F8F98F16B7289B72790C189DC59FE839C959ED2D61E33CDBE7B9B384C334457CB44615364C06D3CC47764F4B2280F07F9C8244EF3C0CED74F09593E876E"
  }
}
