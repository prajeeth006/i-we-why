{
    "segment-control": {
        "default": {
            "color": {
                "bg": {
                    "$extensions": {
                        "com.entaingroup.ext-figma": {
                            "collection": "segment-control",
                            "status": "active"
                        }
                    },
                    "$value": "{semantic.color.opacity.base}"
                },
                "border": {
                    "$extensions": {
                        "com.entaingroup.ext-figma": {
                            "collection": "segment-control",
                            "status": "active"
                        }
                    },
                    "$value": "{semantic.color.opacity.transparent}"
                }
            }
        },
        "inverse": {
            "color": {
                "bg": {
                    "$extensions": {
                        "com.entaingroup.ext-figma": {
                            "collection": "segment-control",
                            "status": "active"
                        }
                    },
                    "$value": "{semantic.color.opacity.base-inverse}"
                },
                "border": {
                    "$extensions": {
                        "com.entaingroup.ext-figma": {
                            "collection": "segment-control",
                            "status": "active"
                        }
                    },
                    "$value": "{semantic.color.opacity.transparent}"
                }
            }
        },
        "shared": {
            "radius": {
                "border": {
                    "$extensions": {
                        "com.entaingroup.ext-figma": {
                            "collection": "segment-control",
                            "status": "active"
                        }
                    },
                    "$value": "{semantic.radius-l2.segmented-control}"
                }
            },
            "size": {
                "tap-target": {
                    "$extensions": {
                        "com.entaingroup.ext-figma": {
                            "collection": "segment-control",
                            "status": "active"
                        }
                    },
                    "$value": "{semantic.size.x8}"
                }
            },
            "space": {
