{
    "switch": {
        "disabled": {
            "color": {
                "knob-off": {
                    "$extensions": {
                        "com.entaingroup.ext-figma": {
                            "collection": "switch",
                            "status": "active"
                        }
                    },
                    "$value": "{semantic.color.on-disabled.base}"
                },
                "knob-on": {
                    "$extensions": {
                        "com.entaingroup.ext-figma": {
                            "collection": "switch",
                            "status": "active"
                        }
                    },
                    "$value": "{semantic.color.on-disabled.base}"
